<!DOCTYPE html>
<html lang="en">
  {% include 'partials/head.njk' %}
<body>
  <header>
  	<a href="#main-content" id="visually-hidden">Skip to main content</a>
    <a href='#' class="stt" title="scroll to top"></a>
    <p class="brand-name"><a href="/" class="brand-link">{% raw %}{{ 11tybundle.dev }}{% endraw %}</a></p>
    {% include "partials/headernav.njk" %}
  </header>
  {% if site.env == "development" %}<p>{{ site.env }}</p>{% endif %}
  <main id="main-content">
    {# provide per-page styling via front matter #}
    {% if pageId %}
      <div id="{{ pageId }}">
        {{ content | safe }}
      </div>
    {% else %}
      {{ content | safe }}
    {% endif %}
  </main>
  <footer>
    {% include 'partials/bundleresources.njk' %}
    {% include "partials/footernav.njk" %}
  </footer>
</body>
</html>
