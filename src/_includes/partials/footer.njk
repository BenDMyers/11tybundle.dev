<div class="buy-beer"><script type="text/javascript" src="https://cdnjs.buymeacoffee.com/1.0.0/button.prod.min.js" data-name="bmc-button" data-slug="11tybundle" data-color="#6096b4" data-emoji="🍺"  data-font="Lato" data-text="Buy me a beer" data-outline-color="#ffffff" data-font-color="#ffffff" data-coffee-color="#FFDD00" ></script></div>
<footer data-pagefind-ignore>
  {% include 'partials/bundleresources.njk' %}
{#
  Note: the header and footer include different navigation.
#}
  <nav>
    <ul class="brand-nav">
      {% for link in site.footerNavLinks %}
        <li>
          <a href="{{ link.url }}" {% if link.rel %}rel="{{ link.rel }}" title="{{link.title}}" type="{{link.type}}" {% endif %}class="nav-link" {{ link.text | isCurrentPage(page.url) | safe }}>{{ link.text }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
</footer>
