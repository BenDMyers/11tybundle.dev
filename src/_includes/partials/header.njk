<header>
  <a href="#main-content" id="visually-hidden">Skip to main content</a>
  {# <a href='#' class="stt" title="scroll to top"></a> #}
  {# if we're in dev, say so on every page #}
  <a href="/" class="brand-link">{% raw %}{{ 11tybundle.dev }}{% endraw %}</a>
  {# {% if site.env == "development" %}<p>{{ site.env }}</p>{% endif %} #}
{#
  Note: the header and footer include different navigation.
#}
  <nav>
    <ul class="brand-nav">
      {% for link in site.mainNavLinks %}
        <li>
          <a href="{{ link.url }}" class="nav-link" {{ link.text | isCurrentPage(page.url) | safe }}>{{ link.text }}</a>
        </li>
      {% endfor %}
    </ul>
  </nav>
</header>
